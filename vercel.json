{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "google-site-verification",
          "value": "Qnsd9LEcigygldPR6_eQv6B-uw81pZnrHHzYPwM_cRg"
        }
      ]
    }
  ]
}
